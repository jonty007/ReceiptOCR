    "compression": "^1.7.4",
    "connect-modrewrite": "^0.10.2",
    "convert-excel-to-json": "^1.6.1",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "crypto": "^1.0.1",
    "dot": "^1.1.3",
    "dotenv": "^8.2.0",
    "easy-soap-request": "^3.2.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^4.0.1",
    "gulp": "^4.0.2",
    "he": "^1.2.0",
    "i18n": "^0.10.0",
    "jszip": "^3.2.2",
    "jwt-simple": "^0.5.6",
    "method-override": "^3.0.0",
    "mime-types": "^2.1.26",
