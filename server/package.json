{
  "name": "test-app",
  "version": "0.0.1",
  "description": "A starter server application",
  "main": "index.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "apidoc": {
    "title": "Server starter API Docs",
    "url": "http://localhost:4001/v1",
    "sampleUrl": "http://localhost:4001/v1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "export NODE_ENV=local && npm run migrate && gulp serve",
    "start:dev": "export NODE_ENV=development && gulp serve",
    "migrate": "node_modules/.bin/sequelize db:migrate && node_modules/.bin/sequelize db:seed:all",
    "server": "export NODE_ENV=local && npm run migrate && npm run start",
    "server:dev": "export NODE_ENV=development && npm run migrate && npm run start",
    "lint": "npm run precommit-msg && eslint ./ --fix --cache --ignore-pattern .gitignore",
    "pm2:local": "NODE_ENV=local pm2 start ./start.js --name=app-server",
    "pm2:development": "NODE_ENV=development pm2 start ./start.js --name=app-server",
    "precommit-msg": "echo 'Server: Pre-commit lint checks...' && exit 0",
    "build:docs": "apidoc  -i ./ -e node_modules/ -o ./dist/apidoc/"
  },
  "author": "test",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.2.1",
    "@google-cloud/vision": "^2.1.2",
    "app-module-path": "^2.2.0",
